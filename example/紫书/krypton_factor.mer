int cnt=0;int n=0,L=0;int S[100]={0};function bool dfs(int cur){    cnt+=1;    if(cnt-1==n)    {        for(int i=0;i<cur;i+=1)        {            std.cout(cast<char>('A'+S[i]));        }        return false;    }    for(int i=0;i<L;i+=1)    {        S[cur]=i;        int ok=1;        for(int j=1;j*2<=cur+1;j+=1)        {            bool equal=true;            for(int k=0;k<j;k+=1)            {                if(S[cur-k]!=S[cur-k-j])                {                    equal=false;                    break;                }            }            if(equal)            {                ok=0;                break;            }        }        if(ok==1)        {            if(!dfs(cur+1))		 {                return false;		 }        }    }    return true;}program main{    n=7;    L=3;    dfs(0);}